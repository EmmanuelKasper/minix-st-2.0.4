# Makefile for rlogind.

CC	= acc
OPT	= -O2
CFLAGS	= $(OPT) -D_MINIX -I../../../include
LDFLAGS	=

all:	rlogind

OBJ=	rlogind.o setup.o

rlogind:	$(OBJ)
	$(CC) $(LDFLAGS) -o $@ $(OBJ)
	install -S 5kw $@

install:	/usr/bin/in.rlogind

/usr/bin/in.rlogind:	rlogind
	install -c $? $@

clean:
	rm -f *.o rlogind core a.out

clobber:	clean

# Dependencies.
$(OBJ):		rlogind.h
